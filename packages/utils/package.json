{
  "name": "@gsmlg/utils",
  "private": false,
  "author": "GSMLG",
  "version": "4.0.0-alpha.2",
  "description": "Functions of SingleCloud.",
  "keywords": [
    "singlecloud"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build --tag next",
    "typescript": "tslint -p tsconfig.json \"{src,test}/**/*.{ts,tsx}\""
  },
  "main": "./src/index.js",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "@material-ui/system": "^4.0.0-alpha.0",
    "@material-ui/utils": "^4.0.0-alpha.2",
    "@types/jss": "^9.5.6",
    "@types/react-transition-group": "^2.0.16",
    "brcast": "^3.0.1",
    "clsx": "^1.0.2",
    "crypto-js": "^3.1.9-1",
    "csstype": "^2.5.2",
    "debounce": "^1.1.0",
    "dom-helpers": "^3.2.1",
    "hoist-non-react-statics": "^3.2.1",
    "is-plain-object": "^3.0.0",
    "jss": "^9.8.7",
    "jss-camel-case": "^6.0.0",
    "jss-default-unit": "^8.0.2",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "jss-props-sort": "^6.0.0",
    "jss-vendor-prefixer": "8",
    "normalize-scroll-left": "^0.2.0",
    "popper.js": "^1.14.1",
    "prop-types": "^15.7.2",
    "react-event-listener": "^0.6.6",
    "react-transition-group": "4",
    "warning": "^4.0.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-dev-warning": "^0.1.1"
  }
}
