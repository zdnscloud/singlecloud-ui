{
  "name": "@singlecloud/ui",
  "version": "1.0.0-beta.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/singlecloud/singlecloud-ui.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "GSMLG",
  "license": "MIT",
  "files": [
    "build"
  ],
  "scripts": {
    "analyze:clean": "rm -f stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run build:clean",
    "build": "NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rm -rf ./build",
    "start": "NODE_ENV=development node server",
    "start:tunnel": "NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rm -rf ./coverage",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.3",
    "@loadable/component": "^5.6.1",
    "@material-ui/core": "^4.1.0",
    "@material-ui/icons": "^4.1.0",
    "@vx/gradient": "^0.0.189",
    "@vx/group": "^0.0.190",
    "@vx/hierarchy": "^0.0.190",
    "@vx/mock-data": "^0.0.189",
    "@vx/scale": "^0.0.190",
    "@vx/shape": "^0.0.190",
    "autoresponsive-react": "^1.1.31",
    "chalk": "^2.4.1",
    "compression": "1",
    "connected-react-router": "6",
    "cross-env": "5.2.0",
    "crypto-js": "^3.1.9-1",
    "express": "4.17.1",
    "fontfaceobserver": "2.1.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "immutable": "4.0.0-rc.12",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "localforage": "^1.7.3",
    "lodash": "4.17.15",
    "minimist": "1.2.0",
    "moment": "^2.24.0",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "15.7.2",
    "react": "16",
    "react-ace": "7.0.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-app-polyfill": "1",
    "react-dom": "16",
    "react-helmet": "5.2.1",
    "react-intl": "2.9.0",
    "react-move": "^6.0.0",
    "react-redux": "7",
    "react-router": "5",
    "react-router-dom": "5",
    "react-spring": "^8.0.19",
    "react-virtualized": "^9.21.0",
    "redux": "4.0.4",
    "redux-form": "^8.1.0",
    "redux-immutable": "4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "4.0.0",
    "rxjs": "^6.4.0",
    "sockjs-client": "^1.3.0",
    "styled-components": "4.3.2",
    "warning": "4.0.3",
    "xterm": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7",
    "@babel/core": "7",
    "@babel/plugin-proposal-class-properties": "7",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7",
    "@babel/plugin-transform-react-constant-elements": "7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "^4.1.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "body-parser": "^1.18.3",
    "circular-dependency-plugin": "5.0.2",
    "compare-versions": "3.5.0",
    "compression-webpack-plugin": "3.0.0",
    "coveralls": "3.0.5",
    "css-loader": "3.1.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "6",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "4.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "imports-loader": "0.8.0",
    "jest-cli": "24.8.0",
    "jest-styled-components": "^7.0.0-2",
    "lint-staged": "9",
    "node-plop": "0.19.0",
    "null-loader": "3.0.0",
    "offline-plugin": "5.0.7",
    "plop": "2.4",
    "pre-commit": "1.2.2",
    "prettier": "1.18",
    "react-test-renderer": "16",
    "rimraf": "2.6.3",
    "shelljs": "^0.8.2",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "svg-url-loader": "3.0.0",
    "terser-webpack-plugin": "1.3.0",
    "url-loader": "2.1.0",
    "webapp-webpack-plugin": "^2.7.1",
    "webpack": "4",
    "webpack-cli": "3",
    "webpack-dev-middleware": "3",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-pwa-manifest": "4.0.0",
    "whatwg-fetch": "3.0.0"
  }
}
